pipeline 
{
agent any
   stages
    {
          stage('Print a massege')
          {
            steps {echo "hello jenkins"}
          }
          stage('print a 2nd massege')
          {
            steps {echo "lo......ollll"}
          }
          stage('print a 3rd massege')
          {
            steps {echo 'helllo..............'}
          }
     }
}
